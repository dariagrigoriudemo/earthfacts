{
  "routes": [
    {
      "route": "/login",
      "serve": "/.auth/login/twitter",
      "allowedRoles" : ["authenticated"],
      "statusCode" : 200
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout",
      "statusCode" : 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
}
